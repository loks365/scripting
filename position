public class GeametriDash : MonoBehaviour 
{ 

    public Transform transform;
    public float speed;
    // Start is called before the first frame update
    void Start()
    {
        
    }

    // Update is called once per 
    void Update()
    {
        transform.position += new Vector3(speed, 1, 2);
    }
}
